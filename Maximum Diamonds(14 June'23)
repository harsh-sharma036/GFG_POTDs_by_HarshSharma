    long long maxDiamonds(int A[], int N, int K) {
        // code here
         priority_queue<int>q;
        for(int i=0; i<N; i++){
            q.push(A[i]);
        }
        long long  sum=0;
        while(K--){
            int ans= q.top();
            q.pop();
            sum= sum+ans;
            q.push(ans/2);
        }
        return sum;
    }
