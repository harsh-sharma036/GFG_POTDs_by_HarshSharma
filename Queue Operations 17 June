    // Function to insert element into the queue
    void insert(queue<int> &q, int k){
        // Your code here
        q.push(k);
    }
    
    // Function to find frequency of an element
    // return the frequency of k
    int findFrequency(queue<int> &q, int k){
        // Your code here
        int freq = 0;
        
        queue <int> temp = q;
        while(!temp.empty()){
            if(temp.front() == k){
                freq++;
            }
            temp.pop();
        }
        
        return freq;
    }
